<?xml version="1.0" encoding="utf-8"?>
<ku.olga.route_builder.presentation.view.SplashLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?colorAccent"
        app:layout_horizontal_percent="50"
        app:layout_vertical_percent="50" />

    <View
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?colorAccent"
        app:layout_horizontal_percent="25"
        app:layout_vertical_percent="50" />

    <View
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?colorAccent"
        app:layout_horizontal_percent="75"
        app:layout_vertical_percent="50" />

    <View
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?colorAccent"
        app:layout_horizontal_percent="100"
        app:layout_vertical_percent="50" />

    <View
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="?colorAccent"
        app:layout_horizontal_percent="0"
        app:layout_vertical_percent="50" />
</ku.olga.route_builder.presentation.view.SplashLayout>